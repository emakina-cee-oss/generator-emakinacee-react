{
    "presets": [[
        "@babel/env", {
            "modules": false,
            "debug": true
        }],
        "@babel/stage-3",
        "@babel/react"
    ],
    "plugins": [
      ["module-resolver", {
        "extensions": [".js", ".jsx"],
        "root": ["./src"]
      }]
    ],
    "env": {
      "test": {
        "presets": [[
          "@babel/env", {
            "modules": "commonjs",
            "targets": {
              "node": "current"
            }
          }],
          "@babel/stage-3",
          "@babel/react"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
  